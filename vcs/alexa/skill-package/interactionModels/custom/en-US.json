    },
    "prompts": [
        {
            "id": "Elicit.Slot.service-type",
            "variations": [
                {"type": "PlainText", "value": "I can {operation} alerts or location services, which one would you like me to {operation}?"}
            ]
        },
        {
            "id": "Slot.Validation.service-type",
            "variations": [
                {"type": "PlainText", "value": "I can't {operation} {service}, I can only {operation} alerts or location services, which one would you like me to {operation}?" }
            ]
        }, 
        {
          "id": "Elicit.Slot.object-type",
          "variations": [
              {"type": "PlainText", "value": "For which object should I {operation} {service}? You can ask me to list objects if unsure."}
          ]
        },
        {
          "id": "Elicit.Slot.channel-type",
          "variations": [
              {"type": "PlainText", "value": "On what channel should I {operation} {service}? You can ask me to list channels if unsure."}
          ]
        },
        {
          "id": "Elicit.Slot.component-type",
          "variations": [
              {"type": "PlainText", "value": "I can list channels, objects or active services. What should I list?"}
          ]
        },
        {
            "id": "Slot.Validation.component-type",
            "variations": [
                {"type": "PlainText", "value": "I can't list {component}, I can only list channels, objects or active services." }
            ]
        }, 
        {
            "id": "Slot.Validation.enable-disable",
            "variations": [
